@import "../../node_modules/normalize.css/normalize.css";
html,
body,
div,
span,
h1,
h2,
h3,
h4,
h5,
h6,
p,
em,
img,
strong,
sub,
sup,
b,
u,
i,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
table,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  vertical-align: baseline;
  background: transparent;
}

* {
  text-decoration: none;
  list-style: none;
  box-sizing: border-box;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

html {
  font-size: 10px;
}

body {
  font-family: "Poppins", sans-serif;
}
body.overflow {
  overflow: hidden;
}

a {
  text-decoration: none;
  display: inline-block;
  cursor: pointer;
  transition: all 0.3s ease;
}

a:hover,
a:active,
a:focus {
  text-decoration: none;
}

input {
  border: none;
  background-color: transparent;
  outline: none;
  -webkit-appearance: none;
}

input[type=number]::-webkit-outer-spin-button,
input[type=number]::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

input[type=number] {
  -moz-appearance: textfield;
}

input[type=number]:hover,
input[type=number]:focus {
  -moz-appearance: number-input;
}

input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

textarea {
  resize: none;
  border: none;
  background-color: transparent;
  outline: none;
  -webkit-appearance: none;
}

button {
  padding: 0;
  cursor: pointer;
  border: none;
  background-color: transparent;
  transition: all 0.3s ease;
  outline: none;
  -webkit-appearance: none;
}

select {
  cursor: pointer;
  border: none;
  background-color: transparent;
  transition: all 0.3s ease;
  outline: none;
  -webkit-appearance: none;
}

.wrapper {
  width: 100%;
  position: relative;
  overflow: hidden;
}

.container {
  max-width: 134.2rem;
  padding: 0 2rem;
  margin: 0 auto;
}

.container-small {
  max-width: 58.7rem;
  padding: 0 2rem;
  margin: 0 auto;
}

.scroll-wrap::-webkit-scrollbar {
  width: 3px;
  background-color: #ccc;
}

.scroll-wrap::-webkit-scrollbar-thumb {
  background-color: rgba(0, 0, 0, 0.75);
  border-radius: 2px;
}

.main-title {
  font-size: 3.2rem;
  line-height: 4.8rem;
  font-weight: 700;
  color: #1f2b38;
}

.section-title {
  font-size: 2.2rem;
  line-height: 3.6rem;
  font-weight: 500;
  color: #1f2b38;
}
@media (min-width: 360px) {
  .section-title {
    font-size: 2.4rem;
  }
}

.user {
  text-align: left;
}
.user__name {
  display: block;
  font-size: 1.8rem;
  line-height: 3rem;
  font-weight: 700;
  color: #1f2b38;
}
@media (min-width: 1310px) {
  .user__name {
    font-size: 2rem;
  }
}
.user__secondname {
  font-size: 1.4rem;
  line-height: 2.1rem;
  color: #ff5693;
}

.header {
  padding: 1.2rem 0;
  border-bottom: 1px solid #e2e2e2;
}
.header .container {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.header .logo {
  width: 5.8rem;
  flex: none;
  margin-right: 2rem;
}
.header .user-wrap {
  display: flex;
}

.nav ul {
  display: flex;
  align-items: center;
  flex-direction: column;
}
@media (min-width: 768px) {
  .nav ul {
    flex-direction: row;
  }
}
.nav li {
  margin-bottom: 3rem;
}
@media (min-width: 768px) {
  .nav li {
    margin-right: 4rem;
    margin-bottom: 0;
  }
  .nav li:last-child {
    margin-right: 0;
  }
}
.nav a {
  font-size: 2.2rem;
  line-height: 1.6;
  color: #ffffff;
  border-bottom: 2px solid transparent;
  transition: all 0.3s ease;
}
@media (min-width: 768px) {
  .nav a {
    font-size: 1.4rem;
    line-height: 1.5;
    color: #1f2b38;
    opacity: 0.5;
  }
}
.nav a:hover, .nav a.active {
  opacity: 1;
  border-bottom: 2px solid #ffffff;
}

.user-nav {
  display: flex;
  align-items: center;
  position: relative;
}
@media (min-width: 768px) {
  .user-nav.user-nav_profile .btn {
    margin-right: 2rem;
  }
}
@media (min-width: 968px) {
  .user-nav.user-nav_profile {
    padding-right: 305px;
  }
  .user-nav.user-nav_profile .btn {
    margin-right: 0;
  }
}
.user-nav .btn {
  display: none;
}
@media (min-width: 768px) {
  .user-nav .btn {
    display: inline-block;
  }
}

.user-info {
  position: absolute;
  top: 0;
  z-index: 4;
  right: 4.1rem;
  width: 300px;
}
@media (min-width: 768px) {
  .user-info {
    right: 0;
  }
}
.user-info__wrap {
  display: flex;
  flex-direction: column;
  overflow: hidden;
  border-radius: 16px;
  max-height: 0;
  transition: all 0.3s ease;
}
@media (min-width: 968px) {
  .user-info__wrap {
    max-height: 4rem;
  }
}
.user-info.active .user-info__wrap {
  background-color: #ffffff;
  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.15);
  max-height: 40rem;
}
.user-info.active .user-info__head {
  padding: 1.6rem 3.7rem 1.2rem 1.6rem;
}
.user-info.active .user-info__head::after {
  transform: rotate(180deg);
  opacity: 1;
}
.user-info__head {
  display: flex;
  align-items: center;
  padding: 0 3.7rem 0 1.6rem;
  background-color: #ffffff;
  transition: all 0.3s ease;
  cursor: pointer;
}
.user-info__head::after {
  content: url("../images/arrow-bottom.svg");
  position: absolute;
  right: 1.9rem;
  top: 2rem;
  transition: all 0.3s ease;
  opacity: 0;
}
@media (min-width: 768px) {
  .user-info__head::after {
    opacity: 1;
  }
}
.user-info__info {
  flex-basis: 0;
  flex-grow: 1;
  overflow: hidden;
}
.user-info__img {
  background-color: #c4c4c4;
  flex-shrink: 0;
  width: 4rem;
  height: 4rem;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 0.8rem;
}
.user-info__img img {
  width: 4rem;
}
.user-info__name {
  display: block;
  font-size: 1.6rem;
  line-height: 1.5;
  font-weight: 700;
  color: #1f2b38;
  white-space: nowrap;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
}
.user-info__description {
  display: flex;
  align-items: center;
}
.user-info__second-name, .user-info__balance {
  font-size: 1.1rem;
  line-height: 1.5;
}
.user-info__second-name {
  color: #ff5693;
  margin-right: 0.8rem;
}
.user-info__balance {
  display: flex;
  align-items: center;
  color: #1f2b38;
}
.user-info__text {
  opacity: 0.5;
  margin-right: 0.2rem;
}
.user-info__icon {
  width: 1.5rem;
  margin-right: 0.2rem;
}
.user-info__icon img {
  display: block;
  max-width: 1.5rem;
}

.user-menu__wrap {
  padding: 0 1.2rem 1.6rem;
  background-color: #ffffff;
}
.user-menu__address {
  display: block;
  font-size: 1.4rem;
  line-height: 1.5;
  color: #1f2b38;
  opacity: 0.5;
  margin-bottom: 0.5rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.user-menu__row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 1.4rem;
  line-height: 1.5;
  color: #1f2b38;
  padding: 0.2rem 0;
  margin-bottom: 0.5rem;
  border-bottom: 1px solid transparent;
  transition: all 0.3s ease;
}
.user-menu__row:hover {
  border-bottom: 1px solid #1f2b38;
}
.user-menu__info {
  color: #ff5e54;
}
.user-menu__out {
  font-size: 1.4rem;
  line-height: 1.5;
  color: #ff5e54;
  transition: all 0.3s ease;
}
.user-menu__out:hover {
  font-weight: 700;
}

.nav-wrap {
  position: fixed;
  z-index: 6;
  top: 0;
  right: -100vw;
  width: 100vw;
  height: 100vh;
  background-color: #ff5693;
  transition: all 0.3s ease;
}
@media (min-width: 768px) {
  .nav-wrap {
    position: static;
    background-color: #ffffff;
    width: auto;
    height: auto;
  }
}
.nav-wrap.active {
  right: 0;
}
.nav-wrap .nav-scroll {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100vh;
  overflow: hidden;
  overflow-y: auto;
  padding: 5rem 2.5rem 3.5rem;
}
@media (min-width: 768px) {
  .nav-wrap .nav-scroll {
    height: auto;
    padding: 0;
  }
}
.nav-wrap .light {
  display: inline-block;
}
@media (min-width: 768px) {
  .nav-wrap .light {
    display: none;
  }
}

.user-burger {
  position: relative;
  padding-right: 1.6rem;
  margin-right: 2rem;
  cursor: pointer;
}
.user-burger::after {
  content: url("../images/arrow-bottom.svg");
  position: absolute;
  right: 0rem;
  top: 50%;
  transform: translateY(-50%);
}
@media (min-width: 768px) {
  .user-burger {
    margin-right: 0;
  }
}
@media (min-width: 968px) {
  .user-burger {
    display: none;
  }
}
.user-burger__img {
  width: 4rem;
  height: 4rem;
  overflow: hidden;
}
.user-burger__img img {
  width: 4rem;
}

.burger {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  position: relative;
  z-index: 10;
  background-color: #ff5693;
  border-radius: 50%;
  width: 4rem;
  height: 4rem;
}
@media (min-width: 768px) {
  .burger {
    display: none;
  }
}
.burger.active span:nth-child(1) {
  top: 7px;
  transform: rotate(45deg);
}
.burger.active span:nth-child(2) {
  transform: translateX(2000px);
}
.burger.active span:nth-child(3) {
  bottom: 7px;
  transform: rotate(-45deg);
}
.burger span {
  width: 20px;
  height: 2px;
  border-radius: 100px;
  position: relative;
  transition: all 0.3s ease;
  background-color: #ffffff;
}
.burger span + span {
  margin-top: 0.5rem;
}

.auctions-section,
.artworks-section {
  padding-bottom: 8rem;
}

.featured-section {
  padding-bottom: 5.7rem;
}

.section-head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0.8rem 0;
  border-bottom: 1px solid #e2e2e2;
  margin-bottom: 1.6rem;
}
.section-head__link {
  font-size: 1.6rem;
  line-height: 1.5;
  color: #ff5693;
  border-bottom: 1px solid transparent;
  transition: all 0.3s ease;
}
.section-head__link:hover {
  border-bottom: 1px solid #ff5693;
}

.cards-wrap {
  display: flex;
  flex-wrap: wrap;
  margin: 0 -0.8rem;
}

.card {
  padding: 0 0.8em;
  margin-bottom: 1.5rem;
  width: 100%;
  transition: all 0.3s ease;
}
@media (min-width: 646px) {
  .card {
    width: 50%;
  }
}
@media (min-width: 800px) {
  .card {
    width: 33.33%;
  }
}
@media (min-width: 1100px) {
  .card {
    width: 25%;
  }
}
@media (min-width: 1280px) {
  .card {
    width: 20%;
  }
}
.card:last-child {
  margin-bottom: 0;
}
.card.card_profile .card__head {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.card__wrap {
  background: #ffffff;
  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.15);
  border-radius: 16px;
}
.card__head {
  padding: 1.2rem;
  height: 24.8rem;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: top center;
}
.card__head-bottom {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.card__status {
  padding: 0.6rem 1.6rem;
  background: #ffffff;
  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.15);
  border-radius: 8px;
  font-size: 1.4rem;
  line-height: 1.5;
  font-weight: 600;
}
.card__status_approved {
  color: rgba(12, 160, 9, 0.5);
}
.card__status_modern {
  color: #ffba09;
}
.card__status_decl {
  color: #ff5e54;
}
.card__body {
  padding: 1rem 1.2rem 1.2rem;
}
.card__title {
  font-size: 2rem;
  line-height: 1.5;
  font-weight: 700;
  color: #1f2b38;
}
.card__descrioption {
  display: flex;
  justify-content: space-between;
}
.card__subtitle {
  font-size: 1.4rem;
  line-height: 1.5;
  color: #1f2b38;
  opacity: 0.5;
}
.card__sold .card__text {
  margin-right: 0.5rem;
}
.card__text-inner {
  padding-left: 2rem;
}
.card__text {
  display: block;
  font-size: 1.6rem;
  line-height: 1.5;
  font-weight: 700;
  color: #1f2b38;
}
.card__buttons-list {
  display: none;
  margin-bottom: 0.8rem;
}

.card-sold.card-sold_big {
  position: relative;
  display: flex;
  align-items: center;
  margin-bottom: 0.4rem;
}
.card-sold.card-sold_big .card-sold__text {
  font-size: 3.2rem;
  line-height: 1.5;
  margin-right: 0.7rem;
}
.card-sold.card-sold_big .card-sold__list {
  left: 8rem;
  top: 3rem;
}
.card-sold.card-sold_big .card-sold__list.active {
  top: 4rem;
}
.card-sold.card-sold_big .card-sold__img {
  width: 3.2rem;
  margin-right: 1.2rem;
}
.card-sold.card-sold_big .card-sold__img img {
  max-width: 3.2rem;
}
.card-sold__subtitle {
  font-size: 1.4rem;
  line-height: 1.5;
  color: #1f2b38;
  opacity: 0.5;
}
.card-sold__inner {
  position: relative;
  display: flex;
  align-items: center;
}
.card-sold__text {
  margin-right: 0.5rem;
  font-size: 1.6rem;
  line-height: 1.5;
  font-weight: 700;
  color: #1f2b38;
}
.card-sold__btn {
  font-size: 1.6rem;
  line-height: 1.5;
  color: #1f2b38;
  opacity: 0.5;
  text-decoration: underline;
  transition: all 0.3s ease;
}
.card-sold__btn:hover {
  opacity: 1;
}
.card-sold__list {
  position: absolute;
  z-index: -1;
  opacity: 0;
  visibility: hidden;
  left: 4rem;
  top: 1.5rem;
  padding: 1.1rem 1.2rem;
  background: #ffffff;
  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.15);
  border-radius: 16px;
  transition: all 0.3s ease;
}
.card-sold__list.active {
  top: 2.5rem;
  z-index: 10;
  opacity: 1;
  visibility: visible;
}
.card-sold__item {
  font-size: 1.6rem;
  line-height: 1.5;
  font-weight: 700;
  color: #1f2b38;
  margin-bottom: 0.6rem;
  display: flex;
  align-items: center;
}
.card-sold__item:last-child {
  margin-bottom: 0;
}
.card-sold__img {
  margin-right: 0.6rem;
  width: 1.3rem;
}
.card-sold__img img {
  max-width: 1.3rem;
}
.card-sold__icon {
  width: 1.6rem;
  margin-right: 0.6rem;
  display: flex;
  align-items: center;
  justify-content: center;
}
.card-sold__icon img {
  max-width: 1.6rem;
}

.creator-card {
  padding: 0 0.8em;
  margin-bottom: 1.5rem;
  width: 100%;
  transition: all 0.3s ease;
}
@media (min-width: 720px) {
  .creator-card {
    width: 50%;
  }
}
@media (min-width: 1100px) {
  .creator-card {
    width: 33.33%;
  }
}
@media (min-width: 1280px) {
  .creator-card {
    width: 25%;
  }
}
.creator-card:hover {
  transform: translateY(-2%);
}
.creator-card:last-child {
  margin-bottom: 0;
}
.creator-card__wrap {
  background-color: #ffffff;
  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.15);
  border-radius: 16px;
  overflow: hidden;
}
.creator-card__head {
  height: 12.7rem;
  padding: 7.7rem 1.2rem 0.8rem;
  display: flex;
  justify-content: space-between;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.creator-card__img {
  border-radius: 50%;
  border: 10px solid #ffffff;
  width: 7rem;
  height: 7rem;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 1rem;
}
@media (min-width: 1310px) {
  .creator-card__img {
    width: 9rem;
    height: 9rem;
    margin-top: 0;
  }
}
.creator-card__img img {
  width: 7rem;
  object-fit: contain;
}
@media (min-width: 1310px) {
  .creator-card__img img {
    width: 9rem;
  }
}
.creator-card__follows {
  display: flex;
}
.creator-card__follow {
  margin-right: 1rem;
}
.creator-card__follow:last-child {
  margin-right: 0;
}
@media (min-width: 1310px) {
  .creator-card__follow {
    margin-right: 2rem;
  }
}
.creator-card__follow span {
  display: block;
  color: #ffffff;
}
.creator-card__follow-title {
  font-size: 1.2rem;
}
@media (min-width: 1310px) {
  .creator-card__follow-title {
    font-size: 1.4rem;
    line-height: 1.5;
  }
}
.creator-card__follow-num {
  font-size: 1.6rem;
  line-height: 1.5;
  font-weight: 700;
}
.creator-card__body {
  padding: 3.6rem 1.2rem 1.2rem;
}
@media (min-width: 1310px) {
  .creator-card__body {
    padding: 4.6rem 1.2rem 1.2rem;
  }
}
.creator-card__info {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 1rem;
}
.creator-card__text {
  font-size: 1.4rem;
  line-height: 1.5;
  color: #1f2b38;
}

.card-menu {
  width: 3.3rem;
  height: 3.2rem;
  background-color: #ffffff;
  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.15);
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  transition: all 0.3s ease;
  margin-bottom: 0.8rem;
  position: relative;
  overflow: hidden;
}
.card-menu:last-child {
  margin-bottom: 0;
}
.card-menu:hover {
  background-color: rgba(255, 255, 255, 0.5);
}
.card-menu:hover span {
  background-color: #ffffff;
}
.card-menu.active {
  background-color: rgba(255, 255, 255, 0.5);
}
.card-menu.active span {
  background-color: #ffffff;
  margin-bottom: 0;
  width: 2rem;
  height: 0.3rem;
  position: absolute;
  top: 50%;
  left: 50%;
}
.card-menu.active span:nth-child(1) {
  transform: translate(-50%, -50%) rotate(45deg);
}
.card-menu.active span:nth-child(2) {
  opacity: 0;
}
.card-menu.active span:nth-child(3) {
  transform: translate(-50%, -50%) rotate(-45deg);
}
.card-menu span {
  width: 0.3rem;
  height: 0.3rem;
  background-color: #1f2b38;
  margin-bottom: 0.4rem;
  transition: all 0.3s ease;
}
.card-menu span:last-child {
  margin-bottom: 0;
}

.card-user {
  display: flex;
  align-items: center;
}
.card-user__img {
  border: 1px solid rgba(255, 255, 255, 0.3);
  border-radius: 50%;
  width: 2.5rem;
  height: 2.5rem;
  overflow: hidden;
  margin-right: 0.5rem;
}
.card-user__img img {
  object-fit: contain;
}
.card-user__name {
  font-size: 1.4rem;
  line-height: 1.5;
  color: #ffffff;
}
