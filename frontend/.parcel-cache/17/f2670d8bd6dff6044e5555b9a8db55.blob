}

.no-csspointerevents .nice-select.open .list {
  display: block;
}

.btn {
  border-radius: 99px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-weight: 500;
  padding: 0.8rem 3rem;
  font-size: 1.4rem;
  line-height: 1.5;
  transition: padding 0.1s ease;
  position: relative;
}
.btn.loading {
  padding: 0.8rem 2rem 0.8rem 4rem;
}
.btn.loading .spinner {
  position: absolute;
  left: 3rem;
  top: 8px;
  animation: rotate 2s linear infinite;
  margin-right: 10px;
  width: 20px;
  height: 20px;
}
.btn.loading .spinner .path {
  stroke: white;
  stroke-linecap: round;
  animation: spinner-dash 1.5s ease-in-out infinite;
}
.btn.primary {
  background-color: #ff5693;
  line-height: 1.5;
  border: 1.5px solid #ff5693;
  color: #ffffff;
}
.btn.primary:hover, .btn.primary.active, .btn.primary:checked {
  color: #ff5693;
  background-color: transparent;
}
.btn.primary:hover.loading .spinner .path, .btn.primary.active.loading .spinner .path, .btn.primary:checked.loading .spinner .path {
  stroke: #ff5693;
}
.btn.secondary {
  color: #ff5693;
  border: 1.5px solid #ff5693;
}
.btn.secondary:hover, .btn.secondary.active, .btn.secondary:checked {
  color: #ffffff;
  background-color: #ff5693;
}
.btn.secondary:hover.loading .spinner .path, .btn.secondary.active.loading .spinner .path, .btn.secondary:checked.loading .spinner .path {
  stroke: #ffffff;
}
.btn.light {
  color: #ffffff;
  border: 1px solid #ffffff;
}
.btn.light:hover, .btn.light.active, .btn.light:checked {
  color: #1f2b38;
  background-color: #ffffff;
}
.btn.light:hover.loading .spinner .path, .btn.light.active.loading .spinner .path, .btn.light:checked.loading .spinner .path {
  stroke: #1f2b38;
}
.btn[disabled] {
  cursor: default;
  opacity: 0.5;
  pointer-events: none;
}

@keyframes rotate {
  100% {
    transform: rotate(360deg);
  }
}
@keyframes spinner-dash {
  0% {
    stroke-dasharray: 1, 150;
    stroke-dashoffset: 0;
  }
  50% {
    stroke-dasharray: 90, 150;
    stroke-dashoffset: -35;
  }
  100% {
    stroke-dasharray: 90, 150;
    stroke-dashoffset: -124;
  }
}
svg.loader {
  width: 60px;
  height: 60px;
}
svg.loader circle {
  fill: transparent;
  stroke: #ff5693;
  stroke-width: 4;
  stroke-linecap: round;
  stroke-dasharray: 188.4;
  -webkit-transform-origin: 30px 30px 0;
  -moz-transform-origin: 30px 30px 0;
  -ms-transform-origin: 30px 30px 0;
  -o-transform-origin: 30px 30px 0;
  transform-origin: 30px 30px 0;
  -webkit-animation: spinner 2s linear infinite;
  -moz-animation: spinner 2s linear infinite;
  -ms-animation: spinner 2s linear infinite;
  -o-animation: spinner 2s linear infinite;
  animation: spinner 2s linear infinite;
}

@keyframes spinner {
  0% {
    transform: rotate(0deg);
    stroke-dashoffset: 39.6;
  }
  50% {
    transform: rotate(720deg);
    stroke-dashoffset: 188.4;
  }
  100% {
    transform: rotate(1080deg);
    stroke-dashoffset: 39.6;
  }
}
